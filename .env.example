# Document Processing Gateway Configuration

# Service settings
SERVICE_NAME=doc-service
VERSION=1.0.0
DEBUG=false
LOG_LEVEL=INFO

# Redis
REDIS_URL=redis://:password@host.docker.internal:6379/1
CELERY_BROKER_URL=redis://:password@host.docker.internal:6379/1
CELERY_RESULT_BACKEND=redis://:password@host.docker.internal:6379/2

# Storage
STORAGE_BASE_PATH=/tmp/document_processing
S3_ENDPOINT=http://host.docker.internal:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET=documents

# Ollama Configuration (model settings are managed via Settings UI / system_settings DB table)
OLLAMA_BASE_URL=http://host.docker.internal:11434

# Internal API (model settings from api-admin)
ADMIN_INTERNAL_URL=http://host.docker.internal:8003
INTERNAL_API_SECRET=change-me-in-production

# api-admin connection (for callbacks)
ADMIN_SERVICE_URL=http://host.docker.internal:8003

# Authentication
JWKS_URL=http://host.docker.internal:8002/.well-known/jwks.json
JWT_AUDIENCE=fastapi-api
JWT_ISSUER=https://auth.example.com

# CORS
CORS_ORIGINS=["*"]
